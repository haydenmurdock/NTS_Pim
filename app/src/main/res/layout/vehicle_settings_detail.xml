<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#303030"
        android:descendantFocusability="beforeDescendants"
        android:focusableInTouchMode="true">

    <TextView
            android:id="@+id/auth_square_textView"
            android:layout_width="163dp"
            android:layout_height="19dp"
            android:text="Authorize Square"
            android:textColor="@color/whiteTextColor"
            tools:layout_editor_absoluteX="502dp"
            tools:layout_editor_absoluteY="474dp" />

    <Button
            android:id="@+id/check_bluetooth_btn"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:text="SQUARE BLUETOOTH CONNECTIONS"
            android:textAlignment="center"
            tools:layout_editor_absoluteX="898dp"
            tools:layout_editor_absoluteY="102dp" />

    <Button
            android:id="@+id/setting_detail_back_btn"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:text="WELCOME SCREEN"
            tools:layout_editor_absoluteX="432dp"
            tools:layout_editor_absoluteY="308dp" />

    <Button
            android:id="@+id/exit_app_btn"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:text="EXIT APPLICATION"
            tools:layout_editor_absoluteX="556dp"
            tools:layout_editor_absoluteY="102dp" />

    <Button
            android:id="@+id/power_Off_PIM_btn"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:text="Power OFF PIM"
            tools:layout_editor_absoluteX="757dp"
            tools:layout_editor_absoluteY="308dp" />

    <ImageView
            android:id="@+id/battery_btn"
            android:layout_width="147dp"
            android:layout_height="116dp"
            android:tooltipText="Controls if app runs off of battery power"
            android:visibility="visible"
            app:srcCompat="@drawable/ic_battery_2"
            tools:layout_editor_absoluteX="167dp"
            tools:layout_editor_absoluteY="523dp"
            tools:srcCompat="@drawable/ic_battery_2" />

    <ProgressBar
            android:id="@+id/activity_indicator_vehicle_detail"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="709dp"
            tools:layout_editor_absoluteY="417dp"
            tools:visibility="invisible" />

    <ImageView
            android:id="@+id/dev_mode_btn"
            android:layout_width="73dp"
            android:layout_height="51dp"
            android:visibility="gone"
            app:srcCompat="@drawable/ic_wrench"
            tools:layout_editor_absoluteX="1203dp"
            tools:layout_editor_absoluteY="29dp" />

    <Button
            android:id="@+id/recent_trip_button"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:text="Recent Trip"
            android:visibility="visible"
            tools:layout_editor_absoluteX="244dp"
            tools:layout_editor_absoluteY="102dp" />

    <ImageButton
            android:id="@+id/vehicle_settings_unpair_button"
            android:layout_width="137dp"
            android:layout_height="126dp"
            android:background="@null"
            android:src="@drawable/ic_broken_link"
            android:text="Unpair"
            tools:layout_editor_absoluteX="1062dp"
            tools:layout_editor_absoluteY="512dp" />

    <ImageView
            android:id="@+id/square_test_image_view"
            android:layout_width="79dp"
            android:layout_height="57dp"
            android:tooltipText="Starts Square Payment for $1.00"
            app:srcCompat="@drawable/ic_square_icon"
            tools:layout_editor_absoluteX="1120dp"
            tools:layout_editor_absoluteY="308dp" />

    <ImageButton
            android:id="@+id/upload_logs_btn"
            android:layout_width="229dp"
            android:layout_height="136dp"
            android:background="@null"
            android:tooltipText="send logs"
            app:srcCompat="@drawable/ic_upload_white"
            tools:layout_editor_absoluteX="737dp"
            tools:layout_editor_absoluteY="523dp" />

    <ImageButton
            android:id="@+id/reauth_btn"
            android:layout_width="147dp"
            android:layout_height="116dp"
            android:background="@null"
            android:tooltipText="re-authorize square"
            app:srcCompat="@drawable/ic_reauth_square"
            tools:layout_editor_absoluteX="487dp"
            tools:layout_editor_absoluteY="522dp" />

    <Button
            android:id="@+id/redo_startup_btn"
            android:layout_width="258dp"
            android:layout_height="wrap_content"
            android:text="PEFORM STARTUP"
            tools:layout_editor_absoluteX="87dp"
            tools:layout_editor_absoluteY="308dp" />

    <Button
            android:id="@+id/tablet_details_btn "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tablet info"
            tools:layout_editor_absoluteX="87dp"
            tools:layout_editor_absoluteY="102dp" />

    <TextView
            android:id="@+id/toggle_battery_power_textView"
            android:layout_width="163dp"
            android:layout_height="19dp"
            android:text="Toggle Battery Power"
            android:textColor="@color/whiteTextColor"
            tools:layout_editor_absoluteX="167dp"
            tools:layout_editor_absoluteY="493dp" />


</androidx.constraintlayout.widget.ConstraintLayout>